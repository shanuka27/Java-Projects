    </servlet>
    <servlet-mapping>
    	<servlet-name>addDoc</servlet-name>
    	<url-pattern>/adddoctor</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>addmedicine</servlet-name>
