<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         

	
    <servlet>
    	<servlet-name>userRegister</servlet-name>
    	<servlet-class>user.userRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>userRegister</servlet-name>
    	<url-pattern>/register</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>userLogin</servlet-name>
    	<servlet-class>user.userLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>userLogin</servlet-name>
    	<url-pattern>/login</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>addFeedback</servlet-name>
    	<servlet-class>feedback.addFeedbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>addFeedback</servlet-name>
    	<url-pattern>/addFeedback</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>deletefeedback</servlet-name>
    	<servlet-class>feedback.deletefeedbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>deletefeedback</servlet-name>
    	<url-pattern>/deletefeedback</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>viewfeedback</servlet-name>
    	<servlet-class>feedback.viewfeedbackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>viewfeedback</servlet-name>
    	<url-pattern>/viewfeedback</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>feedbackUpdate</servlet-name>
    	<servlet-class>feedback.feedbackUpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>feedbackUpdate</servlet-name>
    	<url-pattern>/feedbackUpdate</url-pattern>
    </servlet-mapping>


	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

</web-app>